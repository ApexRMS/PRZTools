  <modules>
    <insertModule id="prz_module_main" className="Module_Main" autoLoad="false" caption="PRZ Tools">
      <tabs>
        <tab id="prz_tab_main" caption="PRZ Tools" keytip="z">
          <group refID="prz_group_main" />
        </tab>
      </tabs>
      <groups>
        <group id="prz_group_main" caption="PRZ Tools" keytip="z">
          <button refID="prz_button_workspacesettings" size="large" />
          <button refID="prz_button_loadprzlayers" size="large" />
          <button refID="prz_button_planningunits" size="large" />
        </group>
      </groups>
      <controls>
        <button id="prz_button_workspacesettings" caption="Workspace Settings" className="Button_WorkspaceSettings" keytip="z" loadOnClick="true" smallImage="pack://application:,,,/ArcGIS.Desktop.Resources;component/Images/GenericButtonPurple16.png" largeImage="pack://application:,,,/ArcGIS.Desktop.Resources;component/Images/GenericButtonPurple32.png">
          <tooltip heading="Workspace Settings">Specify the PRZ Tools Workspace Folder, and other neat stuff.<disabledText /></tooltip>
